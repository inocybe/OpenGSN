<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">
	
	<bean id="vmmModelFactory" class="com.greenstarnetwork.models.vmm.VmHostModelFactory"/>

	<!-- Register vmmModel as an OSGi Service -->
	<service ref="vmmModelFactory" interface="com.iaasframework.capabilities.model.IResourceModelFactory">
		<service-properties>
			<entry key="model.name" value="vmmModel"/>
			<entry key="model.version" value="1.0.0"/>
		</service-properties>
	</service>

</blueprint>
